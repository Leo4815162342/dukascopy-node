{
  "name": "dukascopy-node",
  "version": "1.3.1",
  "description": "Node.js library for downloading historical market data",
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "yarn rollup -c",
    "test": "yarn jest",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "precommit": "lint-staged",
    "prepush": "yarn type-check",
    "build-instruments": "yarn ts-node dev-utils/generate-instruments.ts",
    "test:call": "yarn ts-node dev-utils/test-call.ts"
  },
  "author": "leonid.pyrlia",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Leo4815162342/dukascopy-tools/tree/master/packages/dukascopy-node"
  },
  "keywords": [
    "dukascopy",
    "stock-data",
    "forex-data",
    "historical-data",
    "ohlc",
    "tick-data",
    "exchange-rates",
    "commodities",
    "cfd",
    "crypto"
  ],
  "dependencies": {
    "fastest-validator": "0.6.17",
    "lzma-purejs": "^0.9.3",
    "node-fetch": "^2.3.0",
    "python-struct": "^1.0.6"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.1.7",
    "jest-date-mock": "^1.0.8",
    "lint-staged": "^10.5.3"
  },
  "lint-staged": {
    "src/**/*.{ts,js,json}": [
      "yarn lint",
      "yarn format"
    ]
  },
  "jest": {
    "automock": false,
    "setupFiles": [
      "jest-date-mock"
    ]
  }
}
